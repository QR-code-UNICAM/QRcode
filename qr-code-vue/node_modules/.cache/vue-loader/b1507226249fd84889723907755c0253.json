m-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as Demo03StudentApi from '@/api/infra/demo03-inner';\r\nexport default {\r\n  name: \"Demo03GradeForm\",\r\n  components: {\r\n  },\r\n  props:[\r\n    'studentId'\r\n  ],// 学生编号（主表的关联字段）\r\n  data() {\r\n    return {\r\n      // 表单的加载中：1）修改时的数据加载；2）提交的按钮禁用\r\n      formLoading: false,\r\n      // 表单参数\r\n      formData: [],\r\n      // 表单校验\r\n      formRules: {\r\n        studentId: [{ required: true, message: \"学生编号不能为空\", trigger: \"blur\" }],\r\n        name: [{ required: true, message: \"名字不能为空\", trigger: \"blur\" }],\r\n        teacher: [{ required: true, message: \"班主任不能为空\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  watch:{/** 监听主表的关联字段的变化，加载对应的子表数据 */\r\n    studentId:{\r\n      handler(val) {\r\n        // 1. 重置表单\r\n        this.formData = {\r\n          id: undefined,\r\n          studentId: undefined,\r\n          name: undefined,\r\n          teacher: undefined,\r\n        }\r\n        // 2. val 非空，则加载数据\r\n        if (!val) {\r\n          return;\r\n        }\r\n        try {\r\n          this.formLoading = true;\r\n          // 这里还是需要获取一下 this 的不然取不到 formData\r\n          const that = this;\r\n          Demo03StudentApi.getDemo03GradeByStudentId(val).then(function (res){\r\n            const data = res.data;\r\n            if (!data) {\r\n              return\r\n            }\r\n            that.formData = data;\r\n          })\r\n        } finally {\r\n          this.formLoading = false;\r\n        }\r\n      },\r\n      immediate: true\r\n    }\r\n  },\r\n  methods: {\r\n    /** 表单校验 */\r\n    validate(){\r\n      return this.$refs[\"formRef\"].validate();\r\n    },\r\n    /** 表单值 */\r\n    getData(){\r\n      return this.formData;\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]}]}