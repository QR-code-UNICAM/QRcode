     if (node.children && !node.children.length) {\r\n        delete node.children;\r\n      }\r\n      return {\r\n        id: node.id,\r\n        label: node.name,\r\n        children: node.children\r\n      };\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";;;;;;;;AA2PA,IAAAA,cAAA,GAAAC,sBAAA,CAAAC,OAAA;AACAA,OAAA;AACA,IAAAC,QAAA,GAAAD,OAAA;AAAA,IAAAE,QAAA,GAAAC,OAAA,CAAAC,OAAA,GAEA;EACAC,IAAA;EACAC,UAAA;IAAAC,UAAA,EAAAA;EAAA;EACAC,KAAA;IACAC,QAAA;MACAC,IAAA,EAAAC,MAAA;MACAP,OAAA;IACA;IACAQ,OAAA;MACAF,IAAA,EAAAG,KAAA;MACAT,OAAA;IACA;IACAU,KAAA;MACAJ,IAAA,EAAAG,KAAA;MACAT,OAAA;IACA;EACA;EACAW,IAAA,WAAAA,KAAA;IACA;MACAC,KAAA;QACAC,YAAA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAC,SAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAE,KAAA;UAAAJ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAG,UAAA;UAAAL,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAI,YAAA;UAAAN,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAK,eAAA;UAAAP,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAM,SAAA;UAAAR,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAO,YAAA;UAAAT,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAQ,aAAA;UAAAV,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAS,eAAA;UAAAX,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAU,WAAA;UAAAZ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAW,kBAAA;UAAAb,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAY,gBAAA;UAAAd,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;MACAa,MAAA;IACA;EACA;EACAC,KAAA;IACAzB,QAAA;MACA0B,OAAA,WAAAA,QAAAC,GAAA;QAAA,IAAAC,KAAA;QACA;QACA,IAAAD,GAAA,CAAAE,kBAAA;UACA,IAAAC,4BAAA,EAAAH,GAAA,CAAAE,kBAAA,EAAAE,IAAA,WAAAC,GAAA;YACAJ,KAAA,CAAAJ,MAAA,GAAAQ,GAAA,CAAA1B,IAAA;UACA;QACA;MACA;MACA2B,IAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACA,eACAC,UAAA,WAAAA,WAAAC,IAAA;MACA,IAAAA,IAAA,CAAAC,QAAA,KAAAD,IAAA,CAAAC,QAAA,CAAAC,MAAA;QACA,OAAAF,IAAA,CAAAC,QAAA;MACA;MACA;QACAE,EAAA,EAAAH,IAAA,CAAAG,EAAA;QACAC,KAAA,EAAAJ,IAAA,CAAAzC,IAAA;QACA0C,QAAA,EAAAD,IAAA,CAAAC;MACA;IACA;EACA;AACA"}]}