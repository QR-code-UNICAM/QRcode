\n      :inline-message=\"true\"\r\n    >\r\n      <el-table :data=\"formData\" class=\"-mt-10px\">\r\n        <el-table-column label=\"序号\" type=\"index\" width=\"100\" />\r\n        <el-table-column label=\"名字\" min-width=\"150\">\r\n          <template v-slot=\"{ row, $index }\">\r\n            <el-form-item :prop=\"`${$index}.name`\" :rules=\"formRules.name\" class=\"mb-0px!\">\r\n              <el-input v-model=\"row.name\" placeholder=\"请输入名字\" />\r\n            </el-form-item>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"分数\" min-width=\"150\">\r\n          <template v-slot=\"{ row, $index }\">\r\n            <el-form-item :prop=\"`${$index}.score`\" :rules=\"formRules.score\" class=\"mb-0px!\">\r\n              <el-input v-model=\"row.score\" placeholder=\"请输入分数\" />\r\n            </el-form-item>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column align=\"center\" fixed=\"right\" label=\"操作\" width=\"60\">\r\n          <template v-slot=\"{ $index }\">\r\n            <el-link @click=\"handleDelete($index)\">—</el-link>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </el-form>\r\n    <el-row justify=\"center\" class=\"mt-3\">\r\n      <el-button @click=\"handleAdd\" round>+ 添加学生课程</el-button>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as Demo03StudentApi from '@/api/infra/demo03-inner';\r\nexport default {\r\n  name: \"Demo03CourseForm\",\r\n  components: {\r\n  },\r\n  props:[\r\n    'studentId'\r\n  ],// 学生编号（主表的关联字段）\r\n  data() {\r\n    return {\r\n      // 表单的加载中：1）修改时的数据加载；2）提交的按钮禁用\r\n      formLoading: false,\r\n      // 表单参数\r\n      formData: [],\r\n      // 表单校验\r\n      formRules: {\r\n        studentId: [{ required: true, message: \"学生编号不能为空\", trigger: \"blur\" }],\r\n        name: [{ required: true, message: \"名字不能为空\", trigger: \"blur\" }],\r\n        score: [{ required: true, message: \"分数不能为空\", trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n  watch:{/** 监听主表的关联字段的变化，加载对应的子表数据 */\r\n    studentId:{\r\n      handler(val) {\r\n        // 1. 重置表单\r\n        this.formData = []\r\n        // 2. val 非空，则加载数据\r\n        if (!val) {\r\n          return;\r\n        }\r\n        try {\r\n          this.formLoading = true;\r\n          // 这里还是需要获取一下 this 的不然取不到 formData\r\n          const that = this;\r\n          Demo03StudentApi.getDemo03CourseListByStudentId(val).then(function (res){\r\n            that.formData = res.data;\r\n          })\r\n        } finally {\r\n          this.formLoading = false;\r\n        }\r\n      },\r\n      immediate: true\r\n    }\r\n  },\r\n  methods: {\r\n    /** 新增按钮操作 */\r\n    handleAdd() {\r\n      const row = {\r\n        id: undefined,\r\n        studentId: undefined,\r\n        name: undefined,\r\n        score: undefined,\r\n      }\r\n      row.studentId = this.studentId;\r\n      this.formData.push(row);\r\n    },\r\n    /** 删除按钮操作 */\r\n    handleDelete(index) {\r\n      this.formData.splice(index, 1);\r\n    },\r\n    /** 表单校验 */\r\n    validate(){\r\n      return this.$refs[\"formRef\"].validate();\r\n    },\r\n    /** 表单值 */\r\n    getData(){\r\n      return this.formData;\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":";;;;;;;;;AAwCA,IAAAA,gBAAA,GAAAC,uBAAA,CAAAC,OAAA;AAAA,IAAAC,QAAA,GAAAC,OAAA,CAAAC,OAAA,GACA;EACAC,IAAA;EACAC,UAAA,GACA;EACAC,KAAA,GACA,YACA;EAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACA;MACAC,WAAA;MACA;MACAC,QAAA;MACA;MACAC,SAAA;QACAC,SAAA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAV,IAAA;UAAAQ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;QACAC,KAAA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MACA;IACA;EACA;EACAE,KAAA;IAAA;IACAL,SAAA;MACAM,OAAA,WAAAA,QAAAC,GAAA;QACA;QACA,KAAAT,QAAA;QACA;QACA,KAAAS,GAAA;UACA;QACA;QACA;UACA,KAAAV,WAAA;UACA;UACA,IAAAW,IAAA;UACArB,gBAAA,CAAAsB,8BAAA,CAAAF,GAAA,EAAAG,IAAA,WAAAC,GAAA;YACAH,IAAA,CAAAV,QAAA,GAAAa,GAAA,CAAAf,IAAA;UACA;QACA;UACA,KAAAC,WAAA;QACA;MACA;MACAe,SAAA;IACA;EACA;EACAC,OAAA;IACA,