\r\n  }\r\n  if (second > 0) {\r\n    return second + \"秒\";\r\n  } else {\r\n    return 0 + \"秒\";\r\n  }\r\n}\r\n\r\nexport function beginOfDay(date) {\r\n  return new Date(date.getFullYear(), date.getMonth(), date.getDate());\r\n}\r\n\r\nexport function endOfDay(date) {\r\n  return new Date(date.getFullYear(), date.getMonth(), date.getDate(), 23, 59, 59, 999);\r\n}\r\n\r\nexport function betweenDay(date1, date2) {\r\n  date1 = convertDate(date1);\r\n  date2 = convertDate(date2);\r\n  // 计算差值\r\n  return Math.floor((date2.getTime() - date1.getTime()) / (24 * 3600 * 1000));\r\n}\r\n\r\nexport function formatDate(date, fmt) {\r\n  date = convertDate(date);\r\n  const o = {\r\n    \"M+\": date.getMonth() + 1, //月份\r\n    \"d+\": date.getDate(), //日\r\n    \"H+\": date.getHours(), //小时\r\n    \"m+\": date.getMinutes(), //分\r\n    \"s+\": date.getSeconds(), //秒\r\n    \"q+\": Math.floor((date.getMonth() + 3) / 3), //季度\r\n    \"S\": date.getMilliseconds() //毫秒\r\n  };\r\n  if (/(y+)/.test(fmt)) { // 年份\r\n    fmt = fmt.replace(RegExp.$1, (date.getFullYear() + \"\").substr(4 - RegExp.$1.length));\r\n  }\r\n  for (const k in o) {\r\n    if (new RegExp(\"(\" + k + \")\").test(fmt)) {\r\n      fmt = fmt.replace(RegExp.$1, (RegExp.$1.length === 1) ? (o[k]) : ((\"00\" + o[k]).substr((\"\" + o[k]).length)));\r\n    }\r\n  }\r\n  return fmt;\r\n}\r\n\r\nexport function addTime(date, time) {\r\n  date = convertDate(date);\r\n  return new Date(date.getTime() + time);\r\n}\r\n\r\nexport function convertDate(date) {\r\n  if (typeof date === 'string') {\r\n    return new Date(date);\r\n  }\r\n  return date;\r\n}\r\n"],"mappings":";;;;;;;;;;;;;;;;;;;AAAA;AACA;AACA;AACA;AACA;AACA;AACO,SAASA,OAAOA,CAACC,EAAE,EAAE;EAC1B,IAAMC,GAAG,GAAGC,IAAI,CAACC,KAAK,CAACH,EAAE,IAAI,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG,IAAI,CAAC,CAAC;EAClD,IAAMI,IAAI,GAAIF,IAAI,CAACC,KAAK,CAAEH,EAAE,IAAI,EAAE,GAAG,EAAE,GAAG,IAAI,CAAC,GAAGC,GAAG,GAAG,EAAG,CAAC;EAC5D,IAAMI,MAAM,GAAIH,IAAI,CAACC,KAAK,CAAGH,EAAE,IAAI,EAAE,GAAG,